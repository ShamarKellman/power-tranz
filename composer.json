{
    "name": "shamarkellman/power-tranz",
    "autoload": {
        "psr-4": {
            "Shamarkellman\\PowerTranz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shamarkellman\\PowerTranz\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Shamar Kellman",
            "email": "shamarkellman@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/http": "11.*|12.*",
        "illuminate/support": "11.*|12.*",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^3.8",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
